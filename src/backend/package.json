  "version": "1.0.0",
  "description": "Backend service for Incepta - Technology Transfer and Grant Matching Platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "prepare": "husky install",
    "security:audit": "npm audit",
    "docker:build": "docker build -t incepta-backend .",
    "docker:run": "docker run -p 3000:3000 incepta-backend"
